void moveRowRowABoat(int row)
{
  byte tmp[3];

  tmp[0]=picture[0][row][0];
  tmp[1]=picture[0][row][1];
  tmp[2]=picture[0][row][2];
  for(int i=0;i<179;i++)
  {
      picture[i][row][0]=picture[i+1][row][0];
      picture[i][row][1]=picture[i+1][row][1];
      picture[i][row][2]=picture[i+1][row][2];
  }
  picture[179][row][0]=tmp[0];
  picture[179][row][1]=tmp[1];
  picture[179][row][2]=tmp[2];
}

